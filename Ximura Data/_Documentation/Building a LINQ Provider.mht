href=3D"javascript:CopyCode('ctl00_mainContentContainer_ctl66_code');">
<DIV=20
style=3D"BORDER-BOTTOM: 0px; BORDER-LEFT: 0px; WIDTH: auto; HEIGHT: 9px; =
BORDER-TOP: 0px; BORDER-RIGHT: 0px"=20
class=3DLibC_copy_off align=3Dcenter></DIV>Copy Code </A></DIV></DIV>
<DIV style=3D"BACKGROUND-COLOR: #ddd" dir=3Dltr><PRE =
id=3Dctl00_mainContentContainer_ctl66_code class=3DlibCScode =
space=3D"preserve"><SPAN style=3D"COLOR: blue">private</SPAN> QueryInfo =
ConvertToExecutableQuery(Expression query)
{
&nbsp; // Find the query source
&nbsp; DatabaseMetaTable source;
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (!GetSourceTable(query, out =
source))
&nbsp;&nbsp;&nbsp; throw <SPAN style=3D"COLOR: blue">new</SPAN> =
NotSupportedException(&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <SPAN =
style=3D"COLOR: maroon"><SPAN style=3D"COLOR: maroon">"This query =
expression is not supported!"</SPAN></SPAN>);
&nbsp; StringBuilder sb =3D <SPAN style=3D"COLOR: blue">new</SPAN> =
StringBuilder();
&nbsp; <SPAN style=3D"COLOR: blue">bool</SPAN> useDefault =3D <SPAN =
style=3D"COLOR: blue">false</SPAN>;
&nbsp; // SELECT
&nbsp; sb.Append(<SPAN style=3D"COLOR: maroon"><SPAN style=3D"COLOR: =
maroon">"SELECT "</SPAN></SPAN>);
&nbsp; // TOP
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (_takeTranslator !=3D <SPAN =
style=3D"COLOR: blue">null</SPAN> &amp;&amp; =
_takeTranslator.Count.HasValue)
&nbsp; {
&nbsp;&nbsp;&nbsp; useDefault =3D _takeTranslator.UseDefault;
&nbsp;&nbsp;&nbsp; sb.Append(<SPAN style=3D"COLOR: maroon"><SPAN =
style=3D"COLOR: maroon">"TOP "</SPAN></SPAN>);
&nbsp;&nbsp;&nbsp; sb.Append(_takeTranslator.Count);
&nbsp;&nbsp;&nbsp; sb.Append(<SPAN style=3D"COLOR: maroon"><SPAN =
style=3D"COLOR: maroon">" "</SPAN></SPAN>);
&nbsp; }
&nbsp; // PROJECTION
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (_selectTranslator =3D=3D =
<SPAN style=3D"COLOR: blue">null</SPAN> || =
!_selectTranslator.DataMembers.Any())
&nbsp; {
&nbsp;&nbsp;&nbsp; // project on all the mapped columns
&nbsp;&nbsp;&nbsp; _selectTranslator =3D <SPAN style=3D"COLOR: =
blue">new</SPAN> =
ProjectionTranslator(&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; =
_model, source.PersistentDataMembers);
&nbsp; }
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> =
(!_selectTranslator.DataMembers.Any())
&nbsp;&nbsp;&nbsp; throw <SPAN style=3D"COLOR: blue">new</SPAN> =
Exception(&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <SPAN =
style=3D"COLOR: maroon"><SPAN style=3D"COLOR: maroon">"There are no =
items for projection in this query!"</SPAN></SPAN>);
&nbsp; sb.Append(_selectTranslator.ProjectionClause);
&nbsp; // FROM
&nbsp; sb.Append(<SPAN style=3D"COLOR: maroon"><SPAN style=3D"COLOR: =
maroon">" FROM "</SPAN></SPAN>);
&nbsp; sb.AppendLine(source.TableName);
&nbsp; // WHERE
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (_whereTranslator !=3D =
<SPAN style=3D"COLOR: blue">null</SPAN>)
