&nbsp;&nbsp;&nbsp; <SPAN style=3D"COLOR: blue">string</SPAN> where =3D =
_whereTranslator.WhereClause;
&nbsp;&nbsp;&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (!<SPAN =
style=3D"COLOR: blue">string</SPAN>.IsNullOrEmpty(where))
&nbsp;&nbsp;&nbsp; {
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; sb.Append(<SPAN style=3D"COLOR: =
maroon"><SPAN style=3D"COLOR: maroon">"WHERE "</SPAN></SPAN>);
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; sb.AppendLine(where);
&nbsp;&nbsp;&nbsp; }
&nbsp; }
&nbsp; // ORDER BY
&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (_orderByTranslator !=3D =
<SPAN style=3D"COLOR: blue">null</SPAN>)
&nbsp; {
&nbsp;&nbsp;&nbsp; <SPAN style=3D"COLOR: blue">string</SPAN> orderby =3D =
_orderByTranslator.OrderByClause;
&nbsp;&nbsp;&nbsp; <SPAN style=3D"COLOR: blue">if</SPAN> (!<SPAN =
style=3D"COLOR: blue">string</SPAN>.IsNullOrEmpty(orderby))
